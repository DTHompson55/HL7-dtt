

CREATE FILTER MODULE HL7SoapMsgs_Filter
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE SVAL CHAR 'XXX';

 		SET SVAL = Root.SOAP.Body.ns:HL7ReqType.ReqType;
 				
		IF 'ORU' = SVAL THEN
		 RETURN TRUE;
		END IF;
		IF 'ORM' = SVAL THEN
		 RETURN FALSE;
		END IF;

	RETURN UNKNOWN;		
	END;

END MODULE;
