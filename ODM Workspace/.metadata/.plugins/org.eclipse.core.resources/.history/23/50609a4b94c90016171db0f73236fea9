<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>ReflexRule2</name>
  <uuid>a7cbea2a-a6a8-45b1-a582-36552d58abb3</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[definitions
    set 'the observation set' to the Observation Set named "CBC (INCLUDES DIFF/PLT)" in 'The Report' ;
if
    'the observation set' is not null and 
    all of the following conditions are true :
            - The result is high for Observation "30000000" of 'the observation set'
            - The result is low for Observation "RED BLOOD CELL COUNT" of 'the observation set'
            - The result is abnormal for Observation "HEMOGLOBIN" of 'the observation set' ,
then
    add "** Order Reflex Test #2 based on CBC hi, low, normal wbc,rbc,hemo " to 'the Decision Answer' ;
else
    add "No Reflex Test #2 RBC " + the observation value of Observation "RED BLOOD CELL COUNT" of 'The Report' to 'the Decision Answer' ;
    add "No Reflex Test #2 30000000 " + the observation value of Observation "30000000" of 'The Report' to 'the Decision Answer' ;
    add "No Reflex Test #2 HEMO " + the observation value of Observation "HEMOGLOBIN" of 'The Report' to 'the Decision Answer' ;    ]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
